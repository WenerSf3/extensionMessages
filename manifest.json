{
  "manifest_version": 3,
  "name": "Your Extension",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["https://web.whatsapp.com/"],
      "js": ["popup.js"]
    }
  ],
  "permissions": [
    "storage",
    "declarativeContent",
    "activeTab",
    "tabs",
    "scripting"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "logo.png"
    }
  }
}